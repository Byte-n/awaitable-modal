{
  "name": "async-modal-render",
  "version": "0.0.7",
  "description": "A lightweight library that promisifies React modal operations, allowing elegant handling of modal interactions using async/await syntax",
  "keywords": [
    "react",
    "modal",
    "promise",
    "async",
    "await",
    "dialog",
    "popup",
    "promisify",
    "awaitable",
    "typescript"
  ],
  "homepage": "https://github.com/Byte-n/async-modal-render#readme",
  "bugs": {
    "url": "https://github.com/Byte-n/async-modal-render/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Byte-n/async-modal-render"
  },
  "type": "module",
  "license": "MIT",
  "author": "dancingbytes@qq.com",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "deploy": "gh-pages -d docs-dist",
    "dev": " dumi dev",
    "docs:build": "dumi build",
    "docs:preview": "dumi preview",
    "doctor": "father doctor",
    "lint": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install && dumi setup",
    "prepublishOnly": "father doctor && npm run build",
    "start": "npm run dev",
    "test": "vitest"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@byte.n/dumi-theme-antd": "^0.0.1",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.17",
    "@umijs/lint": "^4.0.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser-playwright": "^4.0.0-beta.17",
    "dumi": "^2.4.13",
    "eslint": "^8.23.0",
    "father": "^4.1.0",
    "gh-pages": "^6.3.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "playwright": "^1.52.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.2.17",
    "react-dom": "^18.2.17",
    "typescript": "~5.8.3",
    "vite-plugin-commonjs": "^0.10.4",
    "vitest": "^4.0.0-beta.17",
    "vitest-browser-react": "v2.0.0-beta.4"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
