{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "jsx": "react",
    "baseUrl": "./",
    "target": "ES2015",
    "module": "ESNext",
    "paths": {
      "@@/*": [
        ".dumi/tmp/*"
      ],
      "async-modal-render": [
        "src"
      ]
    },
    "lib": [
      "dom",
      "es5"
    ]
  },
  "include": [
    ".dumirc.ts",
    "src"
  ]
}
